<h1>Timesheets</h1>
<p>Showing <%= @timesheets.count %> total timesheets.</p>
<p><%= link_to "Import Timesheets", upload_timesheets_path %></p>
<table>
  <thead>
    <tr>
      <th>Staff</th>
      <th>Date</th>
      <th>Description</th>
      <th>Service Group</th>
      <th>Hours</th>
    </tr>
  </thead>
  <tbody>
    <%= render @timesheets %>
  </tbody>
</table>
